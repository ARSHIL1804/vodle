{
  "httpd": {
    "enable_cors": true,
    "WWW-Authenticate": "Basic realm=\"app\"", "-comment": "Basic or Other"
  },
  "chttpd": {
    "require_valid_user": true
  },
  "cors": {
    "origins": "*",
    "credentials": true,
    "methods": [
      "GET",
      "PUT",
      "POST",
      "HEAD",
      "DELETE"
    ],
    "headers": [
      "accept",
      "authorization",
      "content-type",
      "origin",
      "referer",
      "x-csrf-token"
    ]
  },
  "couchdb": {
    "max_document_size": 100000,
    "users_db_security_editable": true
  }
}